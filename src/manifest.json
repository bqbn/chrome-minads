{
  "manifest_version": 3,
  "name": "Min Ads",
  "version": "1.0",
  "description": "Minimum ads on any website",
  "permissions": [
    "declarativeNetRequest",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
